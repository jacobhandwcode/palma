---
interface Props {
  title?: string;
  subtitle?: string;
  categories?: string[];
}

const {
  title = 'In the Heart of Miami Beach',
  subtitle = '',
  categories = [
    'HOSPITALITY',
    'NEIGHBORHOOD',
    'RESTAURANTS',
    'SHOPS',
    'WITHIN TEN MINUTES',
  ],
} = Astro.props;
---

<section class="map-section">
  <div class="map-container">
    <div id="googleMap" class="google-map"></div>
  </div>

  <div class="map-content">
    <div class="map-text">
      <h2>{title}</h2>
      {subtitle && <p class="subtitle">{subtitle}</p>}

      <div class="category-list">
        {
          categories.map((category) => (
            <div class="category-item">
              <span>{category}</span>
            </div>
          ))
        }
      </div>
    </div>
  </div>
</section>

