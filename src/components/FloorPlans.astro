  const firstUnitByFloor = {
    penthouse: 'ph_01',
    'levels-4-12': '4-12_01',
    'level-3': '3_301',
  };

  document.addEventListener('DOMContentLoaded', function () {
    const floorButtons = document.querySelectorAll('.floor-button');
    const residenceInfo = document.getElementById('residence-info');
    const floorPlanDisplay = document.getElementById('floor-plan-display');
    let currentActiveUnit = null;

    function initializeDefault() {
      const defaultFloor = 'levels-4-12';
      const defaultUnit = firstUnitByFloor[defaultFloor];
      loadFloorAndSelectFirstUnit(defaultFloor, defaultUnit);
    }

    function loadFloorAndSelectFirstUnit(floorType, firstUnitId) {
      const svgContainer = document.getElementById(`${floorType}-svg`);

      if (svgContainer && svgData[floorType]) {
        svgContainer.innerHTML = svgData[floorType];

        setTimeout(() => {
          addFloorUnitListeners(svgContainer);

          const firstUnit = svgContainer.querySelector(
            `[data-id="${firstUnitId}"]`
          );
          if (firstUnit) {
            selectUnit(firstUnit, firstUnitId);
          }
        }, 100);
      }
    }

    function addFloorUnitListeners(svgContainer) {
      const floorUnits = svgContainer.querySelectorAll('.floor, g[data-id]');
      floorUnits.forEach((unit) => {
        if (!unit.classList.contains('disabled')) {
          unit.addEventListener('mouseenter', function () {
            if (this !== currentActiveUnit) {
              const bg = this.querySelector('.bg');
              if (bg) bg.style.fill = 'rgba(139, 115, 85, 0.2)';
            }
          });

          unit.addEventListener('mouseleave', function () {
            if (this !== currentActiveUnit) {
              const bg = this.querySelector('.bg');
              if (bg) bg.style.fill = 'white';
            }
          });

          unit.addEventListener('click', function () {
            const unitId = this.dataset.id;
            selectUnit(this, unitId);
          });
        }
      });
    }

    function selectUnit(unitElement, unitId) {
      if (currentActiveUnit) {
        currentActiveUnit.classList.remove('active');
        const prevBg = currentActiveUnit.querySelector('.bg');
        if (prevBg) prevBg.style.fill = 'white';
      }

      currentActiveUnit = unitElement;
      unitElement.classList.add('active');
      const bg = unitElement.querySelector('.bg');
      if (bg) bg.style.fill = 'rgba(139, 115, 85, 0.4)';

      showResidenceInfo(unitId);
    }

    floorButtons.forEach((button) => {
      button.addEventListener('click', function () {
        const floorType = this.dataset.floor;
        const svgContainer = document.getElementById(`${floorType}-svg`);
        const isActive = this.classList.contains('active');

        floorButtons.forEach((btn) => {
          btn.classList.remove('active');
          const targetSvg = document.getElementById(`${btn.dataset.floor}-svg`);
          if (targetSvg) targetSvg.style.display = 'none';
        });

        if (isActive) {
          currentActiveUnit = null;
          residenceInfo.style.display = 'none';
          floorPlanDisplay.style.display = 'none';
        } else {
          this.classList.add('active');
          if (svgContainer && svgData[floorType]) {
            svgContainer.style.display = 'block';

            const firstUnitId = firstUnitByFloor[floorType];
            loadFloorAndSelectFirstUnit(floorType, firstUnitId);
          }
        }
      });
    });

    function showResidenceInfo(unitId) {
      const data = residenceData[unitId];
      if (!data) return;

      document.getElementById('residence-title').textContent = data.title;
      document.getElementById('residence-levels').textContent = data.levels;
      document.getElementById('residence-bedrooms').textContent = data.bedrooms;
      document.getElementById('interior-spec').textContent = data.interior;
      document.getElementById('exterior-spec').textContent = data.exterior;
      document.getElementById('total-spec').textContent = data.total;

      const exteriorRow = document.querySelector('.spec-row:nth-child(2)');
      if (data.exterior === '0') {
        exteriorRow.style.display = 'none';
      } else {
        exteriorRow.style.display = 'flex';
      }

      residenceInfo.style.display = 'block';

      const floorPlanContainer = floorPlanDisplay;

      floorPlanContainer.innerHTML = `
        <img id="floor-plan-img" src="/images/floorplans/${unitId}.png" alt="Floor Plan" />
      `;

      const floorPlanImg = document.getElementById('floor-plan-img');

      floorPlanImg.onerror = function () {
        floorPlanContainer.innerHTML = `
          <div style="display: flex; align-items: center; justify-content: center; color: #999; font-size: 0.9rem; min-height: 400px;">
            ${t.floorPlanNotAvailable} ${data.title}
          </div>
        `;
      };

      floorPlanDisplay.style.display = 'block';
    }

    document
      .querySelector('.download-button')
      .addEventListener('click', function () {
        if (currentActiveUnit) {
          const unitId = currentActiveUnit.dataset.id;
          const data = residenceData[unitId];

          if (data && data.pdfUrl) {
            const link = document.createElement('a');
            link.href = data.pdfUrl;
            link.download = `${unitId}_floorplan.pdf`;
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
          } else {
            const link = document.createElement('a');
            link.href = `/images/floorplans/${unitId}.png`;
            link.download = `${unitId}_floorplan.png`;
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
          }
        } else {
          alert(t.selectResidence);
        }
      });

    initializeDefault();
  });
</script>
