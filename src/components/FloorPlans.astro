---
import { getLangFromUrl, useTranslations } from '../i18n/utils';

interface Props {
  lang?: 'en' | 'es' | 'pt';
}

const { lang } = Astro.props;
const currentLang = lang || getLangFromUrl(Astro.url);
const t = useTranslations(currentLang);
---

<section class="floor-plans-container">
  <div class="floor-plans-content">
    <div class="left-column">
      <h1 class="title">{t('floorPlans.title')}</h1>
      <p class="subtitle">
        {t('floorPlans.subtitle')}
      </p>

      <div class="floor-options">
        <div class="floor-option">
          <button class="floor-button" data-floor="penthouse">
            {t('floorPlans.penthouse')}
            <svg
              class="chevron"
              width="12"
              height="8"
              viewBox="0 0 12 8"
              fill="none"
            >
              <path
                d="M1 1L6 6L11 1"
                stroke="currentColor"
                stroke-width="1.5"
                stroke-linecap="round"
                stroke-linejoin="round"></path>
            </svg>
          </button>
          <div class="floor-svg" id="penthouse-svg" style="display: none;">
          </div>
        </div>

        <div class="floor-option">
          <button class="floor-button active" data-floor="levels-4-12">
            {t('floorPlans.levels4to12')}
            <svg
              class="chevron"
              width="12"
              height="8"
              viewBox="0 0 12 8"
              fill="none"
            >
              <path
                d="M1 1L6 6L11 1"
                stroke="currentColor"
                stroke-width="1.5"
                stroke-linecap="round"
                stroke-linejoin="round"></path>
            </svg>
          </button>
          <div class="floor-svg" id="levels-4-12-svg" style="display: block;">
          </div>
        </div>

        <div class="floor-option">
          <button class="floor-button" data-floor="level-3">
            {t('floorPlans.level3')}
            <svg
              class="chevron"
              width="12"
              height="8"
              viewBox="0 0 12 8"
              fill="none"
            >
              <path
                d="M1 1L6 6L11 1"
                stroke="currentColor"
                stroke-width="1.5"
                stroke-linecap="round"
                stroke-linejoin="round"></path>
            </svg>
          </button>
          <div class="floor-svg" id="level-3-svg" style="display: none;"></div>
        </div>
      </div>
    </div>

    <div class="right-column">
      <div class="residence-info" id="residence-info" style="display: block;">
        <h2 class="residence-title" id="residence-title">
          {t('floorPlans.residence')} 01
        </h2>
        <div class="residence-details">
          <p class="levels" id="residence-levels">
            {t('floorPlans.levels4to12')}
          </p>
          <p class="bedrooms" id="residence-bedrooms">
            {t('floorPlans.bedrooms.2bed1bath')}
          </p>
        </div>

        <div class="residence-specs">
          <div class="spec-row">
            <span class="spec-label">{t('floorPlans.specs.interior')}:</span>
            <span class="spec-value" id="interior-spec"
              >620 SQ.FT. | 58 SQ.M.</span
            >
          </div>
          <div class="spec-row">
            <span class="spec-label">{t('floorPlans.specs.exterior')}:</span>
            <span class="spec-value" id="exterior-spec"
              >149 SQ.FT. | 14 SQ.M.</span
            >
          </div>
          <div class="spec-row">
            <span class="spec-label">{t('floorPlans.specs.total')}:</span>
            <span class="spec-value" id="total-spec">769 SQ.FT. | 71 SQ.M.</span
            >
          </div>
        </div>

        <button class="download-button">{t('floorPlans.download')} →</button>
      </div>

      <div
        class="floor-plan-image"
        id="floor-plan-display"
        style="display: block;"
      >
        <img
          id="floor-plan-img"
          src="/images/floorplans/4-12_01.png"
          alt={t('floorPlans.floorPlanAlt')}
        />
      </div>
    </div>
  </div>
  <h3 class="welcome-home">{t('floorPlans.welcome')}</h3>
</section>

<style lang="scss">
  .floor-plan-image {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    border: 1px solid #ddd;

    img {
      max-width: 100%;
      max-height: 100%;
      object-fit: contain;
    }
  }

  .floor-plans-content {
    display: grid;
    grid-template-columns: 1fr 2fr;
    gap: 4rem;
    min-height: 600px;
  }

  .floor-options {
    .floor-option {
      margin-bottom: 1.5rem;
    }

    .floor-svg {
      margin-top: 2rem;

      svg {
        width: 100%;
        max-width: 500px;
        height: auto;

        .floor,
        g {
          cursor: pointer;

          .bg {
            fill: white;
            stroke: #7b705e;
            stroke-width: 0.4px;
            transition: fill 0.3s ease-in-out;
          }

          &:hover .bg {
            fill: rgba(139, 115, 85, 0.2);
          }

          &.active .bg {
            fill: rgba(139, 115, 85, 0.4);
          }
        }

        g {
          .bg {
            fill: white !important;
          }
        }
      }
      @media (max-width: 1024px) {
        width: 50%;
        margin: 2rem auto 0px;
      }
      @media (max-width: 650px) {
        width: 70%;
        margin: 2rem auto 0px;
      }
    }
  }

  .right-column {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    gap: 2rem;
    @media (max-width: 1024px) {
      flex-direction: column-reverse;
    }
  }

  .residence-info {
    background: #f9f9f9;
    padding: 2rem;

    .residence-title {
      font-size: 1.8rem;
      font-weight: 300;
      letter-spacing: 0.1em;
      color: #8b7355;
      margin: 0 0 1rem 0;
      text-transform: uppercase;
    }

    .residence-details {
      margin-bottom: 2rem;

      .levels {
        font-size: 0.9rem;
        color: #666;
        margin: 0 0 0.5rem 0;
      }

      .bedrooms {
        font-size: 0.9rem;
        color: #333;
        margin: 0;
      }
    }

    .residence-specs {
      margin-bottom: 2rem;

      .spec-row {
        display: flex;
        justify-content: space-between;
        margin-bottom: 0.5rem;
        font-size: 0.9rem;

        .spec-label {
          color: #666;
          font-weight: 500;
        }

        .spec-value {
          color: #333;
        }
      }
    }

    .download-button {
      background: transparent;
      border: 1px solid #8b7355;
      color: #8b7355;
      padding: 0.8rem 1.5rem;
      font-size: 0.9rem;
      font-weight: 500;
      letter-spacing: 0.05em;
      cursor: pointer;
      transition: all 0.3s ease-in-out;
      width: 100%;

      &:hover {
        background: #8b7355;
        color: white;
      }
    }
  }

  .floor-plan-image {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    border: 1px solid #ddd;

    img {
      max-width: 100%;
      max-height: 100%;
      object-fit: contain;
    }
  }

  @media (max-width: 1024px) {
    .floor-plans-content {
      grid-template-columns: 1fr;
      gap: 2rem;
    }
  }

  @media (max-width: 768px) {
    .floor-plans-container {
      padding: 1rem;
    }
  }
</style>

